<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Sequences for menu.item -->
        <record id="seq_menu_item" model="ir.sequence">
            <field name="name">مسلسل الصنف</field>
            <field name="code">menu.item.sequence</field> <!-- unique -->
            <field name="prefix">I</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_menu_item_sale" model="ir.sequence">
            <field name="name">مسلسل امر البيع</field>
            <field name="code">menu.item.sale.sequence</field> <!-- unique -->
            <field name="prefix">S</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_pos_combined_menu_item" model="ir.sequence">
            <field name="name">مسلسل الصنف</field>
            <field name="code">combined.menu.item.sequence</field> <!-- unique -->
            <field name="prefix">I</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_pos_combined_sale_order" model="ir.sequence">
            <field name="name">مسلسل امر البيع</field>
            <field name="code">combined.sale.order.sequence</field> <!-- unique -->
            <field name="prefix">S</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>